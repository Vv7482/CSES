#include <bits/stdc++.h>
using namespace std;
#define int long long
 
void solve() {
    int n;
    cin>>n;
    
    set<int> set;
    for(int i=0 ; i<n ; i++){
        int temp;
        cin>>temp;
        set.insert(temp);
    } 
 
    cout<<set.size()<<'\n';
}
 
signed main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
 
    solve();
    
}
