#include <bits/stdc++.h>
using namespace std;
#define int long long
 
void solve() {
    int n, x;
    cin >> n >> x;
 
    vector<pair<int,int>> a(n);
    for(int i = 0; i < n; i++){
        int temp;
        cin>>temp;
        a[i] = {temp,i+1};
    }
 
    sort(a.begin(), a.end());
    int i = 0, j = n-1;
    bool check = true;
    while(i < j) {
        if(a[i].first + a[j].first == x) {
            check = false;
            cout << a[i].second << ' ' << a[j].second << '\n';
	    	break;	
        } else if(a[i].first + a[j].first < x) {
            i++;
        } else {
            j--;
        }
    }
 
    if(check) cout << "IMPOSSIBLE" << '\n';
}
 
signed main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
 
    solve();
}
