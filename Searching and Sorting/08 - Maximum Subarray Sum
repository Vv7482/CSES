//Kadane's Algorithm
#include <bits/stdc++.h>
using namespace std;
#define int long long
 
void solve() {
	int n;
	cin >> n;
	vector<int> a(n);
    	for(int i = 0; i < n; i++) cin>>a[i];

	int currsum=a[0] , maxsum=a[0];
	for(int i=1 ; i<n ; i++){
		currsum = max(a[i] , currsum+a[i]);
		maxsum = max(maxsum , currsum);
	}
	cout<<maxsum<<'\n';	
}
 
signed main() {
    	ios_base::sync_with_stdio(0);
    	cin.tie(0);
    	cout.tie(0);
 
    	solve();
}
