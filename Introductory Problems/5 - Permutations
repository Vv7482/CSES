#include<bits/stdc++.h>
using namespace std;

void solve(){
    int n;
    cin>>n;

    int e = n/2;
    int o = n/2;
    if(n%2) o++;

    if(n==1){cout<<"1\n";}
    else if(n<=3) cout<<"NO SOLUTION\n";
    else{
        for(int i=1 ; i<=e ; i++){
            int a = 2*i;
            cout<<a<<' ';
        }
        for(int i=0 ; i<o ; i++){
            int a = 2*i + 1;
            cout<<a<<' ';
        }
        cout<<'\n';
    } 
    return;
}

signed main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    
    solve();        
}
